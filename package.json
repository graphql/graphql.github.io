    "react": "15.3.1",
    "react-dom": "15.3.1",
    "sane": "^1.2.0",
    "webpack": "^1.12.1"
  },
  "dependencies": {
    "codemirror": "^5.6.0",
    "codemirror-graphql": "^0.5.7",
    "graphql": "^0.7.0",
